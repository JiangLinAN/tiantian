<?xml version="1.0" encoding="utf-8"?>

<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.nore.dao.GoodTypeDAO">
    <select id="findAllType" resultType="com.nore.pojo.GoodType">
        select *
        from t_type
    </select>
    <select id="findTypeById" resultType="com.nore.pojo.GoodType">
        select *
        from t_type
        where id=#{typeId}
    </select>
</mapper>